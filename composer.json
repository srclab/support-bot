{
    "name": "srclab/support-bot",
    "description": "Бот тех.поддержки для автоответа на популярные фразы клиентов",
    "type": "library",
    "license": "Private",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "SrcLab\\SupportBot\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SrcLab\\SupportBot\\SupportBotServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "^5.7"
    }
}
